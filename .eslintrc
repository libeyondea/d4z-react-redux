{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["react-app", "prettier"],
	"plugins": ["react", "prettier"],
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"printWidth": 120,
				"tabWidth": 2,
				"trailingComma": "none",
				"semi": true,
				"singleQuote": true,
				"useTabs": true
			}
		]
	}
}
